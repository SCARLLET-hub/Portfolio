import java.util.*;

public class fibonacci {
    public static void main(String[]hg){
        Scanner sc = new Scanner(System.in);
        int a=0;
        int b=1;
        int n = sc.nextInt();
        int sum=0;
        while(n>0){
            sum+=a;
            a = b;
            b = sum;
            n--;
        }
        System.out.println(sum);
        sc.close();
    }
}
